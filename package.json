    "copy-webpack-plugin": "^5.0.5",
    "css-loader": "^1.0.0",
    "del": "^3.0.0",
    "eslint": "^4.9.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "livereload": "^0.8.2",
    "mocha": "^5.0.4",
    "monk": "^7.1.1",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.4.0",
    "puppeteer": "^1.20.0",
    "style-loader": "^0.23.0",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.1.2",
    "webpack-shell-plugin": "^0.5.0"
  }
}
